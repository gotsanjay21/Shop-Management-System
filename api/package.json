{
  "name": "sql-products-app",
  "version": "1.0.0",
  "description": "MERN-style application using Express.js + MySQL + Sequelize + React + Bootstrap with auth, products, orders, and analytics.",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "seed": "node src/scripts/seed.js",
    "test": "NODE_ENV=test jest --runInBand"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "roots": ["<rootDir>/tests"],
    "moduleFileExtensions": ["js", "json"],
    "verbose": true,
    "clearMocks": true,
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testTimeout": 30000
  },
  "author": "Got Sanjay",
  "license": "MIT"
}
